{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@icons/*": [
        "./src/components/icons/*"
      ],
      "@docs/*": [
        "./src/docs/*"
      ],
    }
  }
}